{
  "name": "taro-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "This is a boilerplate for taro project. You could use it as a base to build your own app.",
  "scripts": {
    "build:weapp": "yarn run lint && taro build --type weapp",
    "build:swan": "yarn run lint && taro build --type swan",
    "build:alipay": "yarn run lint && taro build --type alipay",
    "build:tt": "yarn run lint && taro build --type tt",
    "build:h5": "yarn run lint && taro build --type h5",
    "build:rn": "yarn run lint && taro build --type rn",
    "dev:weapp": "taro build --watch --type weapp",
    "dev:swan": "taro build --watch --type swan",
    "dev:alipay": "taro build --watch --type alipay",
    "dev:tt": "taro build --watch --type tt",
    "dev:h5": "taro build --watch --type h5",
    "dev:rn": "taro build --watch --type rn",
    "start": "yarn && yarn run dev:weapp",
    "lint": "eslint --ext .js,.ts,.tsx --cache --cache-location node_modules/.cache/.eslintcache --format 'node_modules/eslint-friendly-formatter' src"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/components": "1.2.26",
    "@tarojs/router": "1.2.26",
    "@tarojs/taro": "1.2.26",
    "@tarojs/taro-alipay": "1.2.26",
    "@tarojs/taro-h5": "1.2.26",
    "@tarojs/taro-swan": "1.2.26",
    "@tarojs/taro-tt": "1.2.26",
    "@tarojs/taro-weapp": "1.2.26",
    "nerv-devtools": "^1.3.9",
    "nervjs": "^1.3.9"
  },
  "devDependencies": {
    "@tarojs/plugin-babel": "1.2.26",
    "@tarojs/plugin-csso": "1.2.26",
    "@tarojs/plugin-sass": "1.2.26",
    "@tarojs/plugin-uglifyjs": "1.2.26",
    "@tarojs/webpack-runner": "1.2.26",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-taro": "1.2.26",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-taro": "1.2.26",
    "typescript": "^3.0.1"
  }
}
